@page "/build"
@inject BlazorBattles50.Client.Services.IBananaService BananaService
<h3>Build</h3>
<button @onclick="() => EatBananas(10)">Eat Bananas</button>

@code {
    private void EatBananas(int amount){
        BananaService.EatBananas(amount);
        @*Console.WriteLine($"{amount} bananas eaten.");*@
    }
}
